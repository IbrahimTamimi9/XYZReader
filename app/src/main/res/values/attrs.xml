<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BlankView">
        <attr name="ratio" format="float"/>
        <attr name="static_height" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="ParallaxRatioImageView">
        <attr name="image_ratio" format="float"/>
        <attr name="scrimAlpha" format="float"/>
        <attr name="maxScrimAlpha" format="float"/>
        <attr name="parallaxFactor" format="float"/>
        <attr name="scrimColor" format="color" />
    </declare-styleable>


    <declare-styleable name="StaggeredDistanceSlide">
        <attr name="spread" format="integer" />
    </declare-styleable>

    <declare-styleable name="Pinnable">
        <attr name="state_pinned" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ElasticDragDismissFrameLayout">
        <attr name="dragDismissDistance" format="dimension" />
        <attr name="dragDismissFraction" format="float" />
        <attr name="dragDismissScale" format="float" />
        <attr name="dragElasticity" format="float" />
    </declare-styleable>

    <declare-styleable name="ReflowText">
        <attr name="velocity" format="dimension" />
        <attr name="minDuration" format="integer" />
        <attr name="maxDuration" format="integer"/>
        <attr name="staggerDelay" format="integer" />
        <attr name="freezeFrame" format="boolean" />
    </declare-styleable>


    <declare-styleable name="LiftOff">
        <attr name="initialElevation" format="dimension" />
        <attr name="finalElevation" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ForegroundCardView">
        <attr name="android:foreground" />
        <attr name="android:foregroundInsidePadding" />
        <attr name="android:foregroundGravity" />
    </declare-styleable>
</resources>